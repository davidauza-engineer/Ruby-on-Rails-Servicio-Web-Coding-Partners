<p id="notice"><%= notice %></p>

<h1>Tecnologias</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tecnologias.each do |tecnologia| %>
      <tr>
        <td><%= tecnologia.nombre %></td>
        <td><%= link_to 'Show', tecnologia %></td>
        <td><%= link_to 'Edit', edit_tecnologia_path(tecnologia) %></td>
        <td><%= link_to 'Destroy', tecnologia, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tecnologia', new_tecnologia_path %>
